mysql> delimiter $$
mysql> create function getmodel(regno varchar (20))
    -> returns varchar(20)
    -> begin
    -> declare modelname varchar(20);
    -> select model into modelname from vehicle where reg_no=regno;
    -> return(modelname);
    -> end$$
Query OK, 0 rows affected (0.39 sec)

mysql> mysql> select getmodel(reg_no) from owns$$


